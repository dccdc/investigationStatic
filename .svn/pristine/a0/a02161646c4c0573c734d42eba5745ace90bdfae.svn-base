.content {
    display: flex;
    justify-content: center;
    align-items: center;
    padding: 0;
    margin-top: 6px;
  }
  
  .space {
    margin-top: 6px;
    width: 98%;
  }
  
  .divider {
    margin-top: 6px;
  }
  
  .header {
    width: 100%;
    height: 48px;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 200;
    background-color: #82b1ff;
  }
  
  .footer {
    padding: 0;
    width: 100%;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 100px;
  }